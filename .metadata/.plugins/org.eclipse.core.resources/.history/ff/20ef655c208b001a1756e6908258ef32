
class Greeting{
	public void fun(){
		System.out.println("Hello from Greeting class");
	}
}

class India{
	
	Greeting g = new Greeting() {
		public void fun() {
			System.out.println("Hello from Greeting anonymous class");
		}
	};
}


public class AnonymousClass {

	public static void main(String[] args) {
		India ob = new India();
		ob.g.fun();
	}

}
