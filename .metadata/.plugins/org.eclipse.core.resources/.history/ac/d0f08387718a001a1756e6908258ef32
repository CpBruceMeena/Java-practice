import java.util.Scanner;

class Account{
	private int bal;
	public Account(int bal) {
		this.bal = bal;
	}
	
	public boolean isSufficient(int amt) {
		if(amt <= bal) return true;
		else return false;
	}
}

class Customer{
	private Account account;
	public Customer(Account account) {
		
	}
}

